            if GodMode1 then
                ReplStor.Remotes.To_Server.Handle_Initiate_S:FireServer("skil_ting_asd", client,
                    "akaza_bda_compass_needle", 1)
                task.wait()
            end
        until getgenv().GodMode1 == false
    end
end)

